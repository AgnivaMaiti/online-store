# Save exact version of packages
save-exact=true

# Set the package-lock to version 2
package-lock=true
package-lock-only=false

# Disable package-lock.json for CI environments
# package-lock=false

# Set the package manager to npm
engine-strict=true

# Configure the cache directory
cache=${HOME}/.npm-cache

# Configure the tmp directory
tmp=${HOME}/.npm-tmp

# Configure the log level
loglevel=warn

# Configure the registry
# registry=https://registry.npmjs.org/

# Configure the package-lock.json format
lockfile-version=3

# Configure the package.json format
json=true

# Configure the package manager
# package-manager=npm@9.6.7

# Configure the node version
# node-version=18.0.0

# Configure the npm version
# npm-version=9.6.7

# Configure the package-lock.json format
# package-lock-version=3

# Configure the package-lock.json format
# package-lock-only=false

# Configure the package-lock.json format
# prefer-offline=true

# Configure the package-lock.json format
# prefer-online=false

# Configure the package-lock.json format
# progress=true

# Configure the package-lock.json format
# save-prefix=^

# Configure the package-lock.json format
# save=false

# Configure the package-lock.json format
# scripts-prepend-node-path=false

# Configure the package-lock.json format
# strict-ssl=true

# Configure the package-lock.json format
# tag-version-prefix=v

# Configure the package-lock.json format
# timing=false

# Configure the package-lock.json format
# unicode=false

# Configure the package-lock.json format
# update-notifier=true

# Configure the package-lock.json format
# usage=false

# Configure the package-lock.json format
# user-agent=npm/9.6.7 node/v18.0.0 win32 x64

# Configure the package-lock.json format
# userconfig=${HOME}/.npmrc

# Configure the package-lock.json format
# version=false

# Configure the package-lock.json format
# versions=false

# Configure the package-lock.json format
# viewer=less

# Configure the package-lock.json format
# which=2

# Configure the package-lock.json format
# workspaces=false

# Configure the package-lock.json format
# yes=false
